{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "VS Code extension",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceRoot}/vscode"],
      "outFiles": ["${workspaceRoot}/vscode/out/**/*.js"],
      "autoAttachChildProcesses": true
    }
  ]
}
